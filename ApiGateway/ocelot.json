{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Orchestrator/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7007
        }
      ],
      "UpstreamPathTemplate": "/gateway/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "orchestrator"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "orchestrator",
      "Config": [
        {
          "Name": "Orchestrator API",
          "Version": "v1",
          "Url": "https://localhost:7007/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7239",
    "DangerousAcceptAnyServerCertificateValidator": true
  }
}